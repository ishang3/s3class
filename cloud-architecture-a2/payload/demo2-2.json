   "objectactivity":{
      "activity_name":"",
      "regex":""
   },
   "regionactivity":[
      {
         "activity_name":"Active",
         "regex":"(R1)+(E)"
      },
      {
         "activity_name":"Active",
         "regex":"(R2)+(E)"
      },
      {
         "activity_name":"Inactive",
         "regex":"(E)+(R1)"
      },
      {
         "activity_name":"Inactive",
         "regex":"(E)+(R2)"
      }
   ],
   "reports":[
      {
         "type":"activity-report",
         "command":"SELECT trackid,match,COUNT(*) from metadata group by trackid,match order by trackid;"
      },
      {
